{
  "name": "vsce-vione",
	"displayName": "View Image on VSCode Explorer",
	"description": "View Image on VSCode Explorer",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.67.0"
	},
	"license": "SEE LICENSE IN LICENSE.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/IsaraAomi/vsce-vione"
	},
	"icon": "media/vsce_logo.png",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:imageView",
		"onCommand:updateImage"
	],
	"main": "./out/extension.js",
	"keywords": [
		"multi-root ready"
	],
	"contributes": {
		"configuration": [
			{
				"id": "imageList",
				"title": "View Image on VSCode Explorer",
				"properties": {
					"conf.settingsEditor.uniqueEnumArraySetting": {
						"type": "array",
						"description": "List of your favorite images",
						"items": {
							"type": "string"
						},
						"uniqueItems": true
					}
				}
			}
		],
		"views": {
			"explorer": [
				{
					"type": "webview",
					"id": "imageView",
					"name": "My Favorite Image"
				}
			]
		},
		"commands": [
			{
				"command": "updateImage",
				"category": "My Favorite Image",
				"title": "Update Image",
				"icon": "$(sync)"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "updateImage",
					"group": "navigation",
					"when": "view == imageView"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.67.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"eslint": "^8.14.0",
		"glob": "^8.0.1",
		"mocha": "^9.2.2",
		"typescript": "^4.6.4",
		"@vscode/test-electron": "^2.1.3"
	}
}
